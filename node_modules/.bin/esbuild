else
  export NODE_PATH="/Users/annperei/Library/CloudStorage/OneDrive-Microsoft/Documents/DATAVIZ_PROJECTS/layercaketemplates/node_modules/esbuild/bin/node_modules:/Users/annperei/Library/CloudStorage/OneDrive-Microsoft/Documents/DATAVIZ_PROJECTS/layercaketemplates/node_modules/esbuild/node_modules:/Users/annperei/Library/CloudStorage/OneDrive-Microsoft/Documents/DATAVIZ_PROJECTS/layercaketemplates/node_modules:/Users/annperei/Library/CloudStorage/OneDrive-Microsoft/Documents/DATAVIZ_PROJECTS/node_modules:/Users/annperei/Library/CloudStorage/OneDrive-Microsoft/Documents/node_modules:/Users/annperei/Library/CloudStorage/OneDrive-Microsoft/node_modules:/Users/annperei/Library/CloudStorage/node_modules:/Users/annperei/Library/node_modules:/Users/annperei/node_modules:/Users/node_modules:/node_modules:/Users/annperei/Library/CloudStorage/OneDrive-Microsoft/Documents/DATAVIZ_PROJECTS/layercaketemplates/node_modules/.pnpm/node_modules:$NODE_PATH"
